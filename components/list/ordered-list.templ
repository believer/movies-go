package list

import "strconv"

templ Ol() {
	<ol class="list">
		{ children... }
	</ol>
}

templ Number(i int64) {
	<div class="text-right tabular-nums text-content-tertiary">
		{ strconv.FormatInt(i, 10) }
	</div>
}

templ Trailing() {
	<span class="flex items-center gap-x-2 whitespace-nowrap text-sm">
		{ children... }
	</span>
}
